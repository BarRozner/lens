{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build"
        ]
      }
    }
  },
  "namedInputs": {
    "productionFiles": [
      "default",
      "!{projectRoot}/**/*.test.ts",
      "!{projectRoot}/**/*.test.tsx",
      "!{projectRoot}/**/*.md"
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],

      "inputs": [
        "productionFiles",

        {
          "env": "NODE_ENV"
        }
      ]
    },
    "build:app": {
      "dependsOn": [
        "^build"
      ]
    },
    "test:integration": {
      "dependsOn": [
        "^build"
      ]
    },
    "build:docs": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build"
      ]
    },
    "test:unit": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
